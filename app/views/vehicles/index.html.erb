<article>
  <header>
    <% if current_user.admin? %>
      <%= link_to "#{ t 'new_masculine'} #{ Vehicle.model_name.human }", new_vehicle_path %>
    <% end %> 

    <%= form_with url: search_vehicles_path, method: :get do |f| %>
      <%= f.label :query, t('buttons.search_vehicle') %>
      <%= f.text_field :query %>
      <%= f.submit t('buttons.search') %>
    <% end %>
  </header>
</article>